#include<stdio.h>
void main(){
	char a[10][10];
	int i,j;
	for( i=0;i<10;i++){
		for( j=0;j<10;j++){
			if(j<5 && i==j){
				a[i][j]='*';
			}
			else if(j>5 && i+j==9){
				a[i][j]='*';
			}
			else{
				a[i][j]=' ';
		}
		}
	}
	for( i=0;i<10;i++){
		for( j=0;j<10;j++){
		printf("%c",a[i][j]);	
}
printf("\n");}

}
